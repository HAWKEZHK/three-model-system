{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "no-unused-expression": true
  },
  "rules": {
    "member-access": false,
    "no-empty-interface": true,
    "no-parameter-reassignment": true,
    "prefer-for-of": true,
    "forin": true,
    "no-arg": true,
    "no-conditional-assignment": true,
    "no-debugger": false,
    "no-empty": true,
    "no-eval": true,
    "no-invalid-template-strings": true,
    "no-sparse-arrays": true,
    "no-switch-case-fall-through": true,
    "no-unsafe-finally": true,
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "no-var-keyword": true,
    "no-var-requires": false,
    "radix": true,
    "triple-equals": true,
    "use-isnan": true,
    "indent": [true, "spaces", 4],
    "align": [true, "statements", "members", "elements"],
    "array-type": [false],
    "class-name": true,
    "comment-format": [true, "check-space"],
    "import-spacing": true,
    "interface-name": [true, "always-prefix"],
    "jsdoc-format": true,
    "new-parens": true,
    "no-trailing-whitespace": [true, "ignore-comments", "ignore-jsdoc"],
    "no-consecutive-blank-lines": [true, 3],
    "no-unnecessary-initializer": true,
    "variable-name": [false],
    "no-default-export": true,
    "ordered-imports": false,
    "eofline": true,
    "quotemark": [true, "single", "jsx-double" ],
    "semicolon": [true, "always", "ignore-interfaces"],
    "member-ordering": [false],
    "curly": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "one-line": [false],
    "one-variable-per-declaration": [false],
    "jsx-no-lambda": [false],
    "space-within-parens": [true, 0],
    "space-before-function-paren": [true, "asyncArrow"],
    "max-classes-per-file": false,
    "no-console": false,
    "encoding": true,
    "file-name-casing": [true, "kebab-case"],
    "trailing-comma": [false, {"multiline": "always", "singleline": "never"}],
    "callable-types": false,
    "whitespace": [true, "check-branch", "check-operator", "check-module"]
  },
  "rulesDirectory": []
}
